@model IEnumerable<Category>
    <h1>Index Category</h1>
    <table class="table table-bordered table-striped">
        <thead>
            <th>Category Name</th>
            <th>DisplayOrder</th>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Name</td>
                    <td>@item.DisplayOrder</td>
                </tr>
            }
        </tbody>
    </table>